[global]
        server string = samba
        idmap config * : range = 3000-7999
        security = user
        server min protocol = SMB2

        # disable printing services
        #load printers = no
        #printing = bsd
        #printcap name = /dev/null
        #disable spoolss = yes

[ShareNado]
        path = /example-path
        read only = no
        valid users = example-user
        guest ok = no